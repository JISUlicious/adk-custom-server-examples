# ADK Web Server Configuration
# Copy this file to .env and customize as needed

# Server settings
HOST=0.0.0.0
PORT=8000

# Agent directory
AGENTS_DIR=./

# Database URLs
SESSION_DB_URL=sqlite:///session.db
MEMORY_DB_URL=sqlite:///memory.db

# Logging level (debug, info, warning, error)
LOG_LEVEL=info

# Enable ADK Web UI (true/false)
WEB_UI_ENABLED=true

# Enable auto-reload for development (true/false)
RELOAD=false
